pollutantmean <- function(directory, pollutant, id=1:332) {
dataset <- NULL
if(pollutant=="sulfate" || pollutant=="nitrate") {
for(i in id) {
lpad_current_id <- paste(rep("0",each = 3-nchar(i)), sep = "", collapse = "")
filename <- paste(lpad_current_id, as.character(i), c(".csv"), sep = "", collapse = "")
current_file_data <- read.csv(paste(directory,"/",filename, sep = "", collapse = ""))
current_file_data <- current_file_data[!is.na(current_file_data[pollutant]),pollutant]
dataset <- append(dataset, current_file_data)
}
return (mean(dataset))
}
else {
return(NA)
}
}
